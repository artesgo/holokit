{
  "name": "@artesgo/holokit",
  "version": "1.5.2",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./*.scss": {
      "import": "./*.scss",
      "require": "./*.scss"
    }
  },
  "files": [
    "dist",
    "index.scss",
    "myth.scss"
  ],
  "peerDependencies": {
    "svelte": "^3.55.1 || 4.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.31.2",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.22.3",
    "@sveltejs/package": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-scope": "^7.2.0",
    "eslint-visitor-keys": "^3.4.1",
    "highlight.js": "^11.8.0",
    "husky": "^8.0.3",
    "pattern.css": "^1.0.0",
    "pkg-types": "^1.0.3",
    "prettier": "^2.8.5",
    "prettier-plugin-svelte": "^2.10.1",
    "pretty-quick": "^3.1.3",
    "publint": "^0.1.16",
    "sass": "^1.59.3",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.6",
    "svelte-highlight": "^7.3.0",
    "tslib": "^2.5.3",
    "typescript": "^5.0.0",
    "vite": "^4.2.1",
    "vitest": "^0.33.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "description": "A component library that is configurable with css variables to customize the look and feel while offering the functionality of common web components.",
  "main": "dist/index.js",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "gsap": "^3.12.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/artesgo/holokit"
  },
  "author": "artesgo",
  "license": "ISC"
}
